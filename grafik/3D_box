function setup() {
  createCanvas(800, 800, WEBGL);
  angleMode(DEGREES);
}

function draw() {
  background(220);
  
  push();
  translate(-200, -200);
  fill('skyblue')
  rotateX(millis()*0.1);
  rotateY(millis()*0.2);
  rotateZ(millis()*0.01);
  box(200, 100);
  pop();
  
  push();
  translate(200, 200);
  fill('pink')
  rotateX(millis()*0.1);
  rotateY(millis()*0.02);
  rotateZ(millis()*0.01);
  box(200, 100);
  pop();
}
